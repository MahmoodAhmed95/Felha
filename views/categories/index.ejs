<%- include('../partials/navbar.ejs') %>
    <h1 class="titleOne">Choose a category of choice!</h1>

    <main>
        <!-- Start categories -->
        <div class="categories" id="categories">

            <div class="container">
                <% categories.forEach(function (category) { %>

                    <div>
                        <a href="/categories/<%= category._id %>">

                        </a>
                        <div class="box">
                            <i class="bi bi-mortarboard-fill"></i>
                            <h3> <a href="/categories/<%= category._id %>" class="categoryA">
                                    <%= category.name %>
                                </a></h3>
                        </div>
                        </a>
                    </div>
                    <% }) %>
            </div>
        </div>
        <!-- End categories -->
    </main>


    <%- include('../partials/footer.ejs') %>